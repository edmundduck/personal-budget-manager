<ul>
    <%
        if (error_msg) {
            if (Array.isArray(error_msg)) {
                error_msg.forEach((err) => {
    %>
                    <li class="error_msg"><%= err %></li>
    <%
                });
            } else {
    %>
                <li class="error_msg"><%= error_msg %></li>
    <%
            }
        };
    %>
</ul>